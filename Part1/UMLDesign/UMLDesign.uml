              </ownedBehavior>
              <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_yklEhgi4EfG9CvxSKhfZJA" name="getEdgeEndpoints" specification="_yklEgAi4EfG9CvxSKhfZJA">
