                </ownedComment>
                <ownedAttribute xmi:type="uml:Property" xmi:id="_4Vwvcgj5EfG9CvxSKhfZJA" name="board" visibility="private" type="_4VtsIgj5EfG9CvxSKhfZJA" isUnique="false"/>
