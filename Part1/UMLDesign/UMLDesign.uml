              <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_yklEjQi4EfG9CvxSKhfZJA" name="getTileNodes" specification="_yklEhwi4EfG9CvxSKhfZJA">
                <language>JAVA</language>
                <body>// Center tile (ID 0) has nodes 0-5
if (tileID == 0) {
    return new int[] { 0, 1, 2, 3, 4, 5 };
}
// Inner ring tiles (1-6) - simplified
if (tileID >= 1 &amp;&amp; tileID &lt;= 6) {
    int baseNode = (tileID - 1) * 2 + 6;
    return new int[] { (tileID - 1), tileID % 6, baseNode, baseNode + 1 };
}
// Outer ring tiles (7-18) - simplified
if (tileID >= 7 &amp;&amp; tileID &lt;= 18) {
    int baseNode = (tileID - 7) * 3 + 18;
    return new int[] { baseNode, baseNode + 1, baseNode + 2 };
}
return new int[] {};</body>
              </ownedBehavior>
