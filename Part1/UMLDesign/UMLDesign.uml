                <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_4Vnlhgj5EfG9CvxSKhfZJA" name="buildRoad" specification="_4VnlgAj5EfG9CvxSKhfZJA">
                  <language>JAVA</language>
                  <body>if (!roadValidator.isValid(playerID, edgeID)) {
    return false;
}
Map&lt;ResourceType, Integer> cost = BuildingCost.ROAD.getCost();
if (!player.hasResources(cost)) {
    return false;
}
Edge edge = board.getEdge(edgeID);
edge.buildRoad(player);
deductResources(player, cost);
return true;</body>
                </ownedBehavior>
                <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_4VoMlAj5EfG9CvxSKhfZJA" name="buildCity" specification="_4Vnlhwj5EfG9CvxSKhfZJA">
                  <language>JAVA</language>
                  <body>Node node = board.getNode(nodeID);
if (node == null)
    return false;
// Must have settlement at this location
if (!node.hasSettlementBy(playerID)) {
    return false;
}
Map&lt;ResourceType, Integer> cost = BuildingCost.CITY.getCost();
if (!player.hasResources(cost)) {
    return false;
}
node.upgradeToCity(player);
deductResources(player, cost);
// +1 VP for upgrade (total 2 for city)
player.addVictoryPoints(1);
return true;</body>
